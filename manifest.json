{
  "manifest_version": 3,
  "name": "Google Drive Video/Audio Downloader",
  "version": "1.0",
  "description": "Download Google Drive videoplayback video and audio files with maximum quality.",
  "author": "Shayan Abbas",
  "permissions": [
    "webRequest",
    "tabs",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "Download video and audio",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
